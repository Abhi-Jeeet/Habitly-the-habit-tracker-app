{
  "cli": {
    "version": ">= 16.13.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_APPWRITE_ENDPOINT": "",
        "EXPO_PUBLIC_APPWRITE_PROJECTID": "YOUR_PROJECT_ID",
        "EXPO_PUBLIC_APPWRITE_PLATFORM": "",
        "EXPO_PUBLIC_HABITLY_DB": "YOUR_DATABASE_ID",
        "EXPO_PUBLIC_HABITLY_COLLECTION_ID": "YOUR_COLLECTION_ID",
        "EXPO_PUBLIC_HABITLY_COMPLETION_COLLECTION_ID": "YOUR_COMPLETION_COLLECTION_ID"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_APPWRITE_ENDPOINT": "",
        "EXPO_PUBLIC_APPWRITE_PROJECTID": "YOUR_PROJECT_ID",
        "EXPO_PUBLIC_APPWRITE_PLATFORM": "",
        "EXPO_PUBLIC_HABITLY_DB": "YOUR_DATABASE_ID",
        "EXPO_PUBLIC_HABITLY_COLLECTION_ID": "YOUR_COLLECTION_ID",
        "EXPO_PUBLIC_HABITLY_COMPLETION_COLLECTION_ID": "YOUR_COMPLETION_COLLECTION_ID"
      }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_APPWRITE_ENDPOINT": "",
        "EXPO_PUBLIC_APPWRITE_PROJECTID": "YOUR_PROJECT_ID",
        "EXPO_PUBLIC_APPWRITE_PLATFORM": "",
        "EXPO_PUBLIC_HABITLY_DB": "YOUR_DATABASE_ID",
        "EXPO_PUBLIC_HABITLY_COLLECTION_ID": "YOUR_COLLECTION_ID",
        "EXPO_PUBLIC_HABITLY_COMPLETION_COLLECTION_ID": "YOUR_COMPLETION_COLLECTION_ID"
      }
    }
  },
  "submit": {
    "production": {}
  }
}